# ============================================
# Function: Process Command
# ============================================
Make ProcessCommand with command:
    
    # Main menu navigation
    If current_view equals "main"
        If command equals "1"
            Set current_view to "apps"
        End If
        
        If command equals "2"
            Set current_view to "processes"
        End If
        
        If command equals "3"
            Set current_view to "system"
        End If
        
        If command equals "4"
            Set current_view to "files"
        End If
        
        If command equals "5"
            Set current_view to "settings"
        End If
        
        If command equals "6"
            Call ShowQuickActions
        End If
        
        If command equals "0"
            Set launcher_running to false
            Write "ğŸš€ Shutting down launcher..."
        End If
    End If
    
    # Application launcher actions
    If current_view equals "apps"
        If command equals "1"
            Write "ğŸš€ Launching Visual Studio Code..."
            Write "âœ… VSCode opened successfully!"
        End If
        
        If command equals "2"
            Write "ğŸš€ Launching Chrome Browser..."
            Write "âœ… Chrome opened successfully!"
        End If
        
        If command equals "3"
            Write "ğŸš€ Launching File Explorer..."
            Write "âœ… Explorer opened successfully!"
        End If
        
        If command equals "4"
            Write "ğŸš€ Launching Windows Terminal..."
            Write "âœ… Terminal opened successfully!"
        End If
        
        If command equals "5"
            Write "ğŸš€ Launching Task Manager..."
            Write "âœ… Task Manager opened successfully!"
        End If
        
        If command equals "6"
            Write "ğŸš€ Opening Windows Settings..."
            Write "âœ… Settings opened successfully!"
        End If
        
        If command equals "7"
            Write "ğŸš€ Launching Calculator..."
            Write "âœ… Calculator opened successfully!"
        End If
        
        If command equals "8"
            Write "ğŸš€ Launching Notepad..."
            Write "âœ… Notepad opened successfully!"
        End If
        
        If command equals "s"
            Call SearchApplications
        End If
        
        If command equals "b"
            Set current_view to "main"
        End If
    End If
    
    # Process manager actions
    If current_view equals "processes"
        If command equals "r"
            Write "ğŸ”„ Refreshing process list..."
            Write "âœ… Process list updated!"
        End If
        
        If command equals "k"
            Write "Enter process PID to kill: "
            Ask for pid
            Write "âŒ Killing process " plus pid plus "..."
            Write "âœ… Process terminated successfully!"
        End If
        
        If command equals "d"
            Write "ğŸ“Š Process details view (coming soon)"
        End If
        
        If command equals "b"
            Set current_view to "main"
        End If
    End If
    
    # System controls actions
    If current_view equals "system"
        If command equals "1"
            Write "ğŸ”’ Locking PC..."
            Write "âœ… PC locked"
        End If
        
        If command equals "2"
            Write "ğŸ˜´ Putting system to sleep..."
            Write "âœ… Sleep mode activated"
        End If
        
        If command equals "3"
            Write "ğŸ”„ Restarting system..."
            Write "âš ï¸  System will restart in 10 seconds"
        End If
        
        If command equals "4"
            Write "âš¡ Shutting down system..."
            Write "âš ï¸  System will shut down in 10 seconds"
        End If
        
        If command equals "5"
            Write "ğŸ‘‹ Signing out..."
            Write "âœ… User signed out"
        End If
        
        If command equals "6"
            Write "ğŸ“Š Opening Task Manager..."
            Write "âœ… Task Manager opened"
        End If
        
        If command equals "b"
            Set current_view to "main"
        End If
    End If
    
    # File manager actions
    If current_view equals "files"
        If command equals "1"
            Write "ğŸ“‚ Opening Home Directory..."
            Write "âœ… C:\\Users\\habib"
        End If
        
        If command equals "2"
            Write "ğŸ“‚ Opening Documents..."
            Write "âœ… C:\\Users\\habib\\Documents"
        End If
        
        If command equals "o"
            Write "Enter folder path: "
            Ask for folder_path
            Write "ğŸ“‚ Opening " plus folder_path
            Write "âœ… Folder opened successfully!"
        End If
        
        If command equals "s"
            Call SearchFiles
        End If
        
        If command equals "b"
            Set current_view to "main"
        End If
    End If
    
    # Settings actions
    If current_view equals "settings"
        If command equals "1"
            Write "ğŸ¨ Toggling theme..."
            Write "âœ… Theme changed to Light"
        End If
        
        If command equals "r"
            Write "ğŸ”„ Resetting to defaults..."
            Write "âœ… Settings reset successfully!"
        End If
        
        If command equals "b"
            Set current_view to "main"
        End If
    End If
    
    # Global commands
    If command equals "help"
        Call ShowHelp
    End If
    
    If command equals "exit"
        Set launcher_running to false
    End If
    
End ProcessCommand

